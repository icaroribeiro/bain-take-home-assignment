{
  "name": "@apps/backend",
  "version": "1.0.0",
  "main": "index.js",
  "author": "√çcaro Ribeiro",
  "license": "ISC",
  "description": "Bain Take-Home Assignment Backend Application",
  "scripts": {
    "lint": "eslint ./src",
    "lint:fix": "eslint --fix ./src",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "gen:swagger": "tsoa spec",
    "gen:routes": "tsoa routes",
    "watch:src": "webpack --config webpack.dev.js",
    "watch:dist": "nodemon --env-file .env.dev ./dist/index.js",
    "test": "jest --coverage",
    "start:dev": "npm-run-all -s gen:swagger gen:routes && npm-run-all -p watch:src watch:dist",
    "build": "npm-run-all -s gen:swagger gen:routes && webpack --config webpack.prod.js"
  },
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "@jest/globals": "^29.7.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/eslint__js": "^8.42.3",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.1.0",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^10.0.0",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^12.0.2",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "jest": "^29.7.0",
    "nodemon": "^3.1.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.4",
    "ts-loader": "^9.5.1",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.0.1",
    "webpack": "^5.93.0",
    "webpack-cli": "^5.1.4",
    "zod-fixture": "^2.5.2"
  },
  "dependencies": {
    "express": "^4.19.2",
    "geolib": "^3.3.4",
    "http-status": "^1.7.4",
    "pg": "^8.12.0",
    "swagger-ui-express": "^5.0.1",
    "tsoa": "^5.1.1",
    "tsyringe": "^4.8.0",
    "typeorm": "^0.3.20",
    "uuid": "^10.0.0",
    "zod": "^3.23.8"
  }
}
